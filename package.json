    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "typescript": "3.5.3",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.1.2",
    "web-ext": "^3.1.1",
    "web-ext-types": "^3.0.0",
    "webpack": "4.29.6",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.0.4",
    "whatwg-fetch": "2.0.3",
    "workbox-webpack-plugin": "4.2.0"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "build:chrome": "cross-env BROWSER=chrome yarn build",
    "build:firefox": "cross-env BROWSER=firefox yarn build",
    "test": "node scripts/test.js --env=jsdom",
    "load": "cd ./build/firefox && web-ext run",
    "load:profile": "cd ./build/firefox && web-ext run -p ../../browser/huser1 --keep-profile-changes --bc",
    "load:old": "cd ./browser/build.old && web-ext run -p ../huser1 --keep-profile-changes --bc",
    "start": "node scripts/start.js"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-restricted-globals": 0,
      "no-useless-escape": 0,
      "no-mixed-operators": 0
    },
    "globals": {
      "browser": "readonly",
      "chrome": "readonly"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
